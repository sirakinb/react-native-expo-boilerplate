# =====================================================================
# Authentication Configuration
# =====================================================================
EXPO_PUBLIC_SUPABASE_URL=your_supabase_url_here
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# =====================================================================
# Push Notifications (optional)
# =====================================================================
EXPO_PUBLIC_PUSH_NOTIFICATION_TOKEN=your_push_notification_token_here

# =====================================================================
# API Keys (optional)
# Add any additional API keys your app needs below
# =====================================================================
EXPO_PUBLIC_API_KEY=your_api_key_here

# =====================================================================
# Feature Flags (optional)
# =====================================================================
EXPO_PUBLIC_ENABLE_ANALYTICS=true
EXPO_PUBLIC_ENABLE_CRASH_REPORTING=true

# =====================================================================
# SUPERWALL PUBLIC API KEYS GO HERE 
# -- find these in your superwall project settings (iOS + Android)
# =====================================================================
EXPO_PUBLIC_SUPERWALL_API_KEY_IOS=your_superwall_ios_key_here
EXPO_PUBLIC_SUPERWALL_API_KEY_ANDROID=your_superwall_android_key_here

# =====================================================================
# OPENAI PROJECT API KEY
# Get your API key from: https://platform.openai.com/api-keys
# Make sure to:
# 1. Have an OpenAI account
# 2. Set up billing
# 3. Create a project API key with access to GPT-4 and GPT-4 Vision
# =====================================================================
EXPO_PUBLIC_OPENAI_API_KEY=your_openai_project_api_key_here
EXPO_PUBLIC_SUPERWALL_API_KEY_ANDROID=your_android_api_key