{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@utils/*": [
        "./utils/*"
      ],
      "@constants/*": [
        "./constants/*"
      ],
      "@types/*": [
        "./types/*"
      ]
    },
    "jsx": "react-jsx",
    "baseUrl": ".",
    "types": [
      "node"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
